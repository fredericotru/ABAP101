REPORT z005_Exercise101

TYPES nome_cliente TYPE c LENGTH 10.

DATA num_empreg TYPE i.

TYPES numero_matricula TYPE n LENGTH 8.
TYPES creation_date TYPE d.
TYPES last_changed TYPE t.

TYPES: BEGIN OF customer_structure,
  name TYPE nome_cliente,
  n_employees LIKE num_empreg,
  matricula_fun TYPE numero_matricula,
  creation_date TYPE d,
  last_changed TYPE t,
END OF customer_structure.
